{#<!DOCTYPE html>#}
<html ng-app="BimotorApp">
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}Welcome!{% endblock %}</title>
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
    {% block stylesheets %}{% endblock %}
    {#<link rel="stylesheet" href="{{ asset('bundles/bimotormain/css/bootstrap-theme.min.css') }}" type="text/css" media="all"/>#}
    <link rel="stylesheet" href="{{ asset('bundles/bimotormain/css/bootstrap.css') }}" type="text/css" media="all"/>
    <link rel="stylesheet" href="{{ asset('bundles/bimotormain/css/styles.css') }}" type="text/css" media="all"/>
</head>
<body>
    {% include '::menu.html.twig' %}
    {% block body %}
        <div class="container" ng-controller="AppController">
            <div class="row">
                {% block content %}{% endblock %}
            </div>
        </div>
    {% endblock %}

    <script>
        var jsBaseParams = {
            'getUsersUrl': '{{ path('get_users') }}',
            'loginUrl': '{{ path('login') }}',
            'uploadDirUsers': '{{ upload_dir_users }}',
            'loginCheckUrl': '{{ path('login_check') }}'
        }
    </script>
    <script src="{{ asset('bundles/bimotormain/js/jquery-1.10.2.min.js') }}"></script>
    <script src="{{ asset('bundles/bimotormain/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('bundles/bimotormain/js/angular.min.js') }}"></script>
    <script src="{{ asset('bundles/bimotormain/js/seed/lib/angular/angular-sanitize.js') }}"></script>
    <script src="{{ asset('bundles/bimotormain/js/seed/js/app.js') }}"></script>
    <script src="{{ asset('bundles/bimotormain/js/seed/js/AppController.js') }}"></script>
    <script src="{{ asset('bundles/bimotormain/js/seed/js/LoginController.js') }}"></script>
    <script src="{{ asset('bundles/bimotormain/js/seed/js/services.js') }}"></script>
    <script src="{{ asset('bundles/bimotormain/js/seed/js/directives.js') }}"></script>
    {% block javascripts %}{% endblock %}
</body>
</html>